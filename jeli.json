{
   "cli": {
      "packageManager": "npm"
   },
   "projects": {
      "mitto": {
         "sourceRoot": "src",
         "prefix": "app",
         "output": {
            "folder": "dist/",
            "view": "index.html",
            "styles": [
               "src/styles.scss"
            ],
            "entryFile": "main.js",
            "copy": [
               {
                  "src": "./src/assets/"
               }
            ]
         },
         "resolve": {},
         "type": "application",
         "styling": "scss",
         "configurations": {
            "production": {
               "buildOptions": {
                  "replace": {
                     "src/environments/env.js": "src/environments/env.prod.js"
                  }
               }
            },
            "serve": {
               "serverOptions": {
                  "port": 4110
               }
            }
         }
      }
   },
   "resolve": {
      "alias": {
          "@jeli/*": "../jeli-js/dist/",
          "@jeli/materials":  "../jeli-materials/dist/fo",
          "@jeli/helpers": "../js-helpers",
          "@jeli/jdb/*": "../jeli-db/dist/"
      }
  },
   "default": "mitto"
}